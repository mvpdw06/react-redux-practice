{
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true,
          "experimentalObjectRestSpread": true
      }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
  },
  "rules": {
    "quotes": [2, "single"],
    "no-undef": "error"
  }
}